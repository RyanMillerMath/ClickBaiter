{
  "manifest_version": 2,
  "name": "ClickBaiter",
  "description": "ClickBaiter flags items that are clickbait from your newsfeed.",
  "version": "0.1",

  "browser_action": {
   "default_popup": "clickbaitpopup.html"
  },
  "permissions": [
   "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.min.js", "clickbaiter.js"],
      "css": ["clickbaiter.css"]
    }
  ]
}